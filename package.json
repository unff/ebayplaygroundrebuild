{
  "name": "ebay-playground-rebuild",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "ng build --base-href ./ && electron .",
    "eprod": "ng build --prod --base-href ./ && electron .",
    "erun": "electron .",
    "pack": "ng build --prod --base-href ./ && build --dir",
    "dist": "ng build --prod --base-href ./ && build",
    "distLinux": "ng build --prod --base-href ./ && build --linux"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@clr/angular": "^0.13.4",
    "@clr/icons": "^0.13.4",
    "@clr/ui": "^0.13.4",
    "@webcomponents/custom-elements": "^1.2.1",
    "core-js": "^2.5.4",
    "ngx-electron": "^1.0.4",
    "ngx-store": "^2.0.0",
    "rxjs": "~6.2.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.4",
    "@angular/cli": "^6.2.4",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^6.1.10",
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "^2.0.5",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "electron": "^2.0.11",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  },
  "build": {
    "appId": "com.electron.foxenlabs.ebayPlayground",
    "productName": "eBay Playground",
    "copyright": "Copyright Â© 2018 JC Ryan",
    "directories": {
      "buildResources": "assets"
    },
    "files": [
      "main.js",
      "build",
      "electron"
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "icons/icon.ico"
    }
  }
}
